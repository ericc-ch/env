{
  "name": "@ericc/env",
  "version": "0.0.4",
  "tasks": {
    "build_npm": {
      "command": "deno run -A scripts/build_npm.ts",
      "description": "Build the bundle for publishing to npm"
    }
  },
  "exports": {
    ".": "./src/main.ts"
  },
  "publish": {
    "exclude": [".vscode/", ".npm", "scripts/"]
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "std-env": "npm:std-env@^3.8.0"
  }
}
